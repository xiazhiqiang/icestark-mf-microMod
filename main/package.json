  "homepage": "https://icestark-react.surge.sh",
  "dependencies": {
    "@alifd/next": "^1.x",
    "@ice/stark-module": "^1.5.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "prop-types": "^15.5.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@ali/eslint-config-att": "^1.0.0",
    "@alifd/theme-design-pro": "^0.x",
    "@iceworks/spec": "^1.0.0",
    "@module-federation/enhanced": "^0.6.15",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "build-plugin-css-assets-local": "^0.1.0",
    "build-plugin-fusion": "^0.1.21",
    "build-plugin-icestark": "^2.0.0",
    "build-plugin-ignore-style": "^0.1.0",
    "build-plugin-js-assets-local": "^0.1.0",
    "build-plugin-moment-locales": "^0.1.0",
    "build-plugin-tpl-render": "^0.1.0",
    "eslint": "^7.30.0",
    "ice.js": "^2.0.0",
    "stylelint": "^13.7.2"
  },
  "scripts": {
    "start": "icejs start --config build.config.js",
    "build": "icejs build --config build.config.js",
    "prepublishOnly": "npm run build",
